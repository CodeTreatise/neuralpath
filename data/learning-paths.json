{
  "learningPaths": [
    {
      "id": "ai-agent-builder",
      "title": "Build AI Agents From Scratch",
      "icon": "ðŸ¤–",
      "description": "Master the complete journey from prompts to autonomous agents",
      "difficulty": "advanced",
      "duration": "12 weeks",
      "goal": "Build autonomous AI systems that can plan, execute, and iterate",
      "targetAudience": "Developers wanting to create intelligent autonomous systems",
      "periodicTableElements": ["Pr", "Fc", "Lg", "Ag"],
      "elementDescriptions": {
        "Pr": "Learn to write system prompts that guide agent behavior",
        "Fc": "Master function calling to give agents tools and actions",
        "Lg": "Understand LLM capabilities that power agent reasoning",
        "Ag": "Build complete agents with ReAct loops"
      },
      "stages": [
        {
          "stageNumber": 1,
          "stageName": "Foundation (Weeks 1-2)",
          "stageDuration": "2 weeks",
          "stageGoal": "Understand LLM basics and how to interact with them",
          "courses": [
            {
              "courseId": "prompt-engineering",
              "courseName": "Prompt Engineering Mastery",
              "why": "Learn to write system prompts - the brain of your agent",
              "duration": "2 weeks",
              "effort": "5-7 hours/week",
              "keyLessons": ["System Prompts", "Few-shot Examples", "Role-based Prompts"],
              "outcome": "Write effective prompts that guide LLM behavior",
              "practicalTask": "Create a system prompt for a data analyst agent"
            }
          ],
          "periodicElements": ["Pr"]
        },
        {
          "stageNumber": 2,
          "stageName": "Core Concepts (Weeks 3-5)",
          "stageDuration": "3 weeks",
          "stageGoal": "Understand function calling and tool use",
          "courses": [
            {
              "courseId": "python-ai",
              "courseName": "Python Fundamentals for AI",
              "why": "You need Python to build agent systems",
              "duration": "2 weeks (review sections)",
              "effort": "3-5 hours/week",
              "keyLessons": ["Functions", "APIs", "Libraries"],
              "outcome": "Comfortable with Python for AI development",
              "prerequisite": true,
              "skip": "If you already know Python well"
            },
            {
              "courseId": "genai-applications",
              "courseName": "GenAI Fundamentals",
              "why": "Understand LLM capabilities and limitations",
              "duration": "3 weeks",
              "effort": "5-7 hours/week",
              "keyLessons": ["Function Calling", "Tool Use", "LLM APIs"],
              "outcome": "Know how to call LLMs and invoke functions",
              "practicalTask": "Build a simple tool-using agent that calls APIs"
            }
          ],
          "periodicElements": ["Fc", "Lg"]
        },
        {
          "stageNumber": 3,
          "stageName": "Building Agents (Weeks 6-10)",
          "stageDuration": "5 weeks",
          "stageGoal": "Build working autonomous agents",
          "courses": [
            {
              "courseId": "agentic-ai",
              "courseName": "Agentic AI & Agent Systems",
              "why": "THE core course - Learn ReAct loops, planning, memory",
              "duration": "5 weeks",
              "effort": "8-10 hours/week",
              "keyLessons": ["ReAct Pattern", "Agent Loops", "Planning Strategies", "Error Handling", "State Management"],
              "outcome": "Build production-grade agents",
              "practicalTasks": [
                "Simple agent with 3 tools",
                "Multi-step planning agent",
                "Agent with memory and context",
                "Error recovery mechanisms"
              ]
            }
          ],
          "periodicElements": ["Ag", "Pr", "Fc"]
        },
        {
          "stageNumber": 4,
          "stageName": "Advanced & Safety (Weeks 11-12)",
          "stageDuration": "2 weeks",
          "stageGoal": "Make agents safe and production-ready",
          "courses": [
            {
              "courseId": "ai-safety-guardrails",
              "courseName": "AI Safety & Guardrails",
              "why": "Agents need safety constraints before production",
              "duration": "2 weeks",
              "effort": "4-6 hours/week",
              "keyLessons": ["Input Validation", "Output Guardrails", "Monitoring"],
              "outcome": "Deploy safe, monitored agents",
              "practicalTask": "Add guardrails to your agent from stage 3"
            }
          ],
          "periodicElements": ["Ag"]
        }
      ],
      "prerequisites": [
        "Basic Python knowledge",
        "Understanding of APIs and HTTP requests",
        "Familiarity with LLMs (ChatGPT, Claude, etc.)"
      ],
      "projectMilestones": [
        {
          "milestone": 1,
          "name": "Simple Agent",
          "week": 7,
          "description": "Build an agent that can call 3 tools and answer questions",
          "periodicElements": ["Pr", "Fc", "Lg"]
        },
        {
          "milestone": 2,
          "name": "Planning Agent",
          "week": 9,
          "description": "Agent that breaks down complex tasks into subtasks",
          "periodicElements": ["Ag"]
        },
        {
          "milestone": 3,
          "name": "Production Agent",
          "week": 12,
          "description": "Complete agent with error handling, guardrails, monitoring",
          "periodicElements": ["Ag", "Gr"]
        }
      ],
      "relatedReactions": ["agent-formula", "safe-agent", "multiagent-formula"],
      "nextSteps": ["Multi-Agent Systems", "Specialized Agents with Fine-tuning"]
    },
